{
  "name": "startease-expressjs-project",
  "version": "1.0.0",
  "description": "Expressjs project generated using startease CLI tool",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "prepare": "husky install",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/index.js",
    "build": "tsc",
    "dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc --watch & nodemon build/index.js",
    "lint": "npx eslint src/",
    "format": "npx prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "mongoose": "^7.6.0",
    "morgan": "^1.10.0",
    "tsconfig-paths": "^3.14.1",
    "winston": "^3.10.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/connect-timeout": "^0.0.37",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/hpp": "^0.2.3",
    "@types/morgan": "^1.9.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "concurrently": "^8.2.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
